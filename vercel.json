{
  "functions": {
    "src/pages/**/*.jsx": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/sitemap.xml",
      "dest": "/sitemap.xml",
      "headers": {
        "content-type": "application/xml"
      }
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt",
      "headers": {
        "content-type": "text/plain"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}